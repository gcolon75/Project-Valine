{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowMediaUploadsPutObject",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:AbortMultipartUpload",
        "s3:ListMultipartUploadParts"
      ],
      "Resource": "arn:aws:s3:::valine-media-uploads/*"
    }
  ]
}
